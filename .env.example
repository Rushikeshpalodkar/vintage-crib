# eBay API Configuration
# Get these from: https://developer.ebay.com/my/keys

# Production Environment
EBAY_APP_ID=your_app_id_here
EBAY_CERT_ID=your_cert_id_here
EBAY_DEV_ID=your_dev_id_here
EBAY_USER_TOKEN=your_user_token_here

# Sandbox Environment (for testing)
EBAY_SANDBOX_APP_ID=your_sandbox_app_id_here
EBAY_SANDBOX_CERT_ID=your_sandbox_cert_id_here
EBAY_SANDBOX_DEV_ID=your_sandbox_dev_id_here
EBAY_SANDBOX_USER_TOKEN=your_sandbox_user_token_here

# Environment (production or sandbox)
EBAY_ENVIRONMENT=sandbox

# eBay AI Features
# Using eBay's built-in AI and recommendation system
# No additional API keys needed for AI features!

# Server Configuration
PORT=3001
NODE_ENV=development