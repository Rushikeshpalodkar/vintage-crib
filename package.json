{
  "name": "vintage-crib",
  "version": "1.0.0",
  "description": "Vintage Crib - A curated vintage products marketplace with stunning 3D design",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "build": "node -e \"const fs=require('fs');const path=require('path');if(!fs.existsSync('public'))fs.mkdirSync('public',{recursive:true});const copyRecursive=(src,dest)=>{const stat=fs.statSync(src);if(stat.isDirectory()){if(!fs.existsSync(dest))fs.mkdirSync(dest);fs.readdirSync(src).forEach(f=>copyRecursive(path.join(src,f),path.join(dest,f)));}else{fs.copyFileSync(src,dest);}};copyRecursive('frontend','public');fs.copyFileSync('index.html','public/index.html');console.log('Build complete!');\"",
    "postinstall": "echo \"Vintage Crib setup complete\""
  },
  "keywords": [
    "vintage",
    "marketplace",
    "curated",
    "3d",
    "ebay",
    "products",
    "ecommerce"
  ],
  "author": "Vintage Crib Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.10.0",
    "bcryptjs": "^3.0.2",
    "cheerio": "^1.1.0",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "dotenv-safe": "^9.1.0",
    "ebay-api": "^9.2.0",
    "express": "^4.18.2",
    "helmet": "^8.1.0",
    "ioredis": "^5.7.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.17.2",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "rate-limiter-flexible": "^7.2.0",
    "redis": "^5.8.1",
    "sharp": "^0.34.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.3",
    "jest": "^30.0.5",
    "mongodb-memory-server": "^10.2.0",
    "nodemon": "^3.0.1",
    "supertest": "^7.1.4"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vintagecrib/website.git"
  },
  "homepage": "https://vintagecrib.com",
  "bugs": {
    "url": "https://github.com/vintagecrib/website/issues"
  }
}
